/control/verbose 0
/tracking/verbose 0

/gps/source/clear
/gps/source/add 1
/gps/particle e-

/gps/pos/type Plane
/gps/pos/shape Circle
/gps/pos/centre -0.67 0.5 0 cm
/gps/pos/radius 0.1 mm
/gps/pos/rot1 1 0 0
/gps/pos/rot2 0 0 1

/gps/ang/type beam1d
/gps/direction 0 -1 0

/gps/ene/type Gauss
/gps/ene/ezero 7 keV 
/gps/ene/sigma 0.5 keV

/run/beamOn 10